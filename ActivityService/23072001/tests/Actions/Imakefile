SOURCES = GlobalAction.java \
	  NormalAction.java

OBJECTS = GlobalAction.class \
	  NormalAction.class

Target($(OBJECTS))

BuildJavaByteCode(GlobalAction,NullParam)
BuildJavaByteCode(NormalAction,NullParam)

Clean(*.class)
