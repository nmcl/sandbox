SetLocalCppFlags(-I../include -I$(ARJUNA_INCLUDES) -DSTD_INCLUDE=\"-I""CPLUSPLUS_INCLUDE_VAR\" -DCOMPILER_NAME=\"CPLUSPLUS_VAR\" -D__EXTERN_C__)

