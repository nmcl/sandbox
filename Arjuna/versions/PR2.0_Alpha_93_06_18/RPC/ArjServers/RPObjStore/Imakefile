SOURCES = RPObjStore.cc DaemonList.cc DaemonData.cc Members.cc RPObjStoreIF.cc

OBJECTS = RPObjStore.o DaemonList.o DaemonData.o Members.o RPObjStoreIF.o

SetLocalCppFlags(-I$(ARJUNA_INCLUDES))

MakeNonRecursiveAllTarget(../libArjServers.a)

MakeNonRecursiveInstall

MakeObjectCPLUSPLUS(RPObjStore,)
MakeObjectCPLUSPLUS(DaemonList,)
MakeObjectCPLUSPLUS(DaemonData,)
MakeObjectCPLUSPLUS(Members,)
MakeObjectCPLUSPLUS(RPObjStoreIF,)

MakeLibrary(../libArjServers.a,$(OBJECTS))

RPObjStore.cc: $(ARJUNA_INCLUDES)/RPC/ArjServers/StateDaemon_stub.h
RPObjStore.cc: $(ARJUNA_INCLUDES)/RPC/ArjServers/StateDaemon_stubclass.h
RPObjStore.cc: $(ARJUNA_INCLUDES)/RPC/ArjServers/NameServ_stub.h
RPObjStore.cc: $(ARJUNA_INCLUDES)/RPC/ArjServers/NameServer_stubclass.h
RPObjStore.cc: $(ARJUNA_INCLUDES)/RPC/ArjServers/RepDesc_stub.h

MakeMakefile

MakeDummyMakefiles

MakeEmptyNonRecursiveTidyCleanPurge

MakeNonRecursiveMakedepend($(SOURCES))

MakeNonRecursiveStripdepend
