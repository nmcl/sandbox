CreateInstallTopDirectory(lib)
InstallLibrary(lib,libArjuna.a)
InstallLibrary(lib,libArjServers.a)
InstallLibrary(lib,libCrashRec.a)
InstallLibrary(lib,libNoneThread.a)
InstallLibrary(lib,libCommon.a)

TidyTarget($(LIBARJUNA) $(LIBARJSERVERS) $(LIBCRASH) $(LIBREPLICATION) $(LIBCOMMON) $(LIBOBJCONFIG) libNoneThread.a)

