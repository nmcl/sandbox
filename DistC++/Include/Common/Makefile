depend.local :: $(SOURCES)
	@CC=$(CPLUSPLUS) $(MAKEDEPEND) $(LOCAL_CPPFLAGS) $(CPPFLAGS) $(LOCAL_MAKEDEPENDFLAGS) $(MAKEDEPENDFLAGS) $(SOURCES)

tidy :: tidy.local

tidy.local ::
	@$(RM) core *~ \#*\# *.bak *.BAK *\%

clean :: clean.local

clean.local :: tidy.local
