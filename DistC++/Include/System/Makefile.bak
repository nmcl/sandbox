all :: all.local
	@for i in $(SUBDIRS)""; \
	do \
	if test -d $$i ; \
	then \
	if test -f $$i/Makefile ; \
	then \
	echo Making $@ in $(CURDIR)/$$i ; \
	(cd $$i; $(MAKE) $(xMAKEFLAGS) MAKE="$(MAKE)" IMAKE="$(IMAKE)" MAKEDEPEND="$(MAKEDEPEND)" CPLUSPLUS=\"$(CPLUSPLUS)\" AR="$(AR)" RANLIB="$(RANLIB)" YACC="$(YACC)" LEX="$(LEX)" MV="$(MV)" CP="$(CP)" RM="$(RM)" CAT="$(CAT)" MKDIR="$(MKDIR)" CHMOD="$(CHMOD)" PURIFY="$(PURIFY)" xMAKEFLAGS="$(xMAKEFLAGS)" IMAKEFLAGS="$(IMAKEFLAGS)" MAKEDEPENDFLAGS="$(MAKEDEPENDFLAGS)" CPPFLAGS="$(CPPFLAGS)" CPLUSPLUSFLAGS="$(CPLUSPLUSFLAGS)" LDFLAGS="$(LDFLAGS)" xLIBRARIES="$(xLIBRARIES)" YACCFLAGS="$(YACCFLAGS)" LEXFLAGS="$(LEXFLAGS)" STUBFLAGS="$(STUBFLAGS)" ARFLAGS="$(ARFLAGS)" CURDIR=$(CURDIR)/$$i all); \
	if test $$? -eq 0 ; then true; else exit 1; fi; \
	else \
echo Ignoring directory with no Makefile: $$i ; \
	fi \
	else \
echo No such directory : $$i ;\
	fi \
	done

