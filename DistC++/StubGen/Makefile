clean :: clean.local

clean.local :: tidy.local
	@$(RM) -f .dummy $(OBJECTS)

purge :: purge.local

purge.local :: clean.local
	@$(RM) -f Makefile $(OBJECTS) *.stamp
	@$(RM) -rf Templates.DB

