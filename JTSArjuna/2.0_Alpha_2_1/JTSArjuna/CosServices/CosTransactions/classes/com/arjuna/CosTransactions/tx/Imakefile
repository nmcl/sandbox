AddToIdlFlags(-DDRAFT5 -I$(JAVACOSTRANSACTIONSTOPDIR)/include)

SOURCES = tx.java \
	  XID.java

OBJECTS = tx.class \
	  XID.class

Target($(SOURCES))
Target($(OBJECTS))

PreprocessJavaCode(tx,NullParam)

BuildJavaByteCode(tx,NullParam)
BuildJavaByteCode(XID,NullParam)

Clean(*.class)
