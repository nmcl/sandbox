public static void randomOperation(int thr, int level)
    {
	switch (Util.rand.nextInt() % 23)
	{
        case 0:
        case 1:
        case 2:
        case 3:
             incr12(thr, level);
            break;
        case 4:
        case 5:
        case 6:
        case 7:
           incr21(thr, level);
            break;
        case 8:
        case 9:
        case 10:
        case 11:
            get12(thr, level);
            break;
        case 12:
        case 13:
