CreateInstallDirectory(/config,/config/ModuleBuildSystem)

InstallFile(/config,Config.def)
InstallFile(/config,ModuleBuildSystem.tmpl)
InstallFile(/config,Macro.def)
InstallFile(/config,Override.def)
InstallFile(/config,Prog.def)
InstallFile(/config,Rule.def)
InstallFile(/config,Sys.def)
InstallProgram(/config,Sys)
InstallProgram(/config,Sys_Version)
InstallFileAs(/config,Module_install.def,Module.def)

Purge(Config.def Local.def ModuleBuildSystem.cf Module.def Module_install.def Sys Sys_Version Sys.def)
