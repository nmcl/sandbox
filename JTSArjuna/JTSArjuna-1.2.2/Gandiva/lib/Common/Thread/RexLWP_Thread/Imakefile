SOURCES = RexLWP_Sem.cc \
	  RexLWP_SpecificData.cc \
	  RexLWP_Thread.cc \
	  RexLWP_ThreadPool.cc

OBJECTS = RexLWP_Sem.o \
	  RexLWP_SpecificData.o \
	  RexLWP_Thread.o \
	  RexLWP_ThreadPool.o

Target($(OBJECTS))

BuildObject(RexLWP_Sem,NullParam,NullParam)
BuildObject(RexLWP_SpecificData,NullParam,NullParam)
BuildObject(RexLWP_Thread,NullParam,NullParam)
BuildObject(RexLWP_ThreadPool,NullParam,NullParam)

AddDependences($(SOURCES),NullParam)
