SOURCES = Single_Sem.cc \
	  Single_SpecificData.cc \
	  Single_Thread.cc \
	  Single_ThreadPool.cc \
	  Single_Condition.cc

OBJECTS = Single_Sem.o \
	  Single_SpecificData.o \
	  Single_Thread.o \
	  Single_ThreadPool.o \
	  Single_Condition.o

Target($(OBJECTS))

BuildObject(Single_Sem,NullParam,NullParam)
BuildObject(Single_SpecificData,NullParam,NullParam)
BuildObject(Single_Thread,NullParam,NullParam)
BuildObject(Single_ThreadPool,NullParam,NullParam)
BuildObject(Single_Condition,NullParam,NullParam)

AddDependences($(SOURCES),NullParam)
