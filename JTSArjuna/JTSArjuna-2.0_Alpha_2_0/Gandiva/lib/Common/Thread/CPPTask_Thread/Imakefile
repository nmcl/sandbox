SOURCES = CPPTask_Sem.cc \
	  CPPTask_SpecificData.cc \
	  CPPTask_Thread.cc \
	  CPPTask_ThreadPool.cc

OBJECTS = CPPTask_Sem.o \
	  CPPTask_SpecificData.o \
	  CPPTask_Thread.o \
	  CPPTask_ThreadPool.o

Target($(OBJECTS))

BuildObject(CPPTask_Sem,NullParam,NullParam)
BuildObject(CPPTask_SpecificData,NullParam,NullParam)
BuildObject(CPPTask_Thread,NullParam,NullParam)
BuildObject(CPPTask_ThreadPool,NullParam,NullParam)

AddDependences($(SOURCES),NullParam)
