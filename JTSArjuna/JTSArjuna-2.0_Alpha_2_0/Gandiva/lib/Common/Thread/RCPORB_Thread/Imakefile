SOURCES = RCPORB_RWLock.cc \
	  RCPORB_Sem.cc \
	  RCPORB_SpecificData.cc \
	  RCPORB_Thread.cc \
	  RCPORB_ThreadPool.cc

OBJECTS = RCPORB_RWLock.o \
	  RCPORB_Sem.o \
	  RCPORB_SpecificData.o \
	  RCPORB_Thread.o \
	  RCPORB_ThreadPool.o

Target($(OBJECTS))

BuildObject(RCPORB_RWLock,NullParam,NullParam)
BuildObject(RCPORB_Sem,NullParam,NullParam)
BuildObject(RCPORB_SpecificData,NullParam,NullParam)
BuildObject(RCPORB_Thread,NullParam,NullParam)
BuildObject(RCPORB_ThreadPool,NullParam,NullParam)

AddDependences($(SOURCES),NullParam)
