SOURCES = Info.java \
	  Configuration.java

OBJECTS = Info.class \
	  Configuration.class

Target($(SOURCES))
Target($(OBJECTS))

PreprocessJavaCode(Configuration,NullParam)
PreprocessJavaCode(Info,NullParam)

BuildJavaByteCode(Configuration,NullParam)
BuildJavaByteCode(Info,NullParam)

Clean(*.class)

