all.local ::
	@echo "Building 'all' in "$(CURDIR)/ArjunaOTS
	@(cd ArjunaOTS ; $(MAKE) CURDIR=$(CURDIR)/ArjunaOTS $(xMAKEFLAGS) all.local)

Makefiles ::
	@echo "Building 'Makefile' in "$(CURDIR)/ArjunaOTS
	@(cd ArjunaOTS ; $(IMAKE) $(IMAKEFLAGS))
	@(cd ArjunaOTS ; $(MAKE) CURDIR=$(CURDIR)/ArjunaOTS $(xMAKEFLAGS) Makefiles)

depend.local ::
	@echo "Constructing 'depend' in "$(CURDIR)/ArjunaOTS
	@(cd ArjunaOTS ; $(MAKE) CURDIR=$(CURDIR)/ArjunaOTS $(xMAKEFLAGS) depend.local)

install.local ::
	@echo "Performing 'install' in "$(CURDIR)/ArjunaOTS
	@(cd ArjunaOTS ; $(MAKE) CURDIR=$(CURDIR)/ArjunaOTS $(xMAKEFLAGS) install.local)

tidy.local ::
	@echo "Performing 'tidy'  in "$(CURDIR)/ArjunaOTS
	@(cd ArjunaOTS ; $(MAKE) CURDIR=$(CURDIR)/ArjunaOTS $(xMAKEFLAGS) tidy.local)

