echo "MODULE_LDLIBRARIES='"$MODULE_LDLIBRARIES"'"
echo "MODULE_LDLIBDEPENDS='"$MODULE_LDLIBDEPENDS"'"
) > config/"$THISMODULE".cf
chmod a+rx config/"$THISMODULE".cf

