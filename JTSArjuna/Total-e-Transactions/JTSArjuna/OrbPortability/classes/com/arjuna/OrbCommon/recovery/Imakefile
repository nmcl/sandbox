SOURCES = IORRecovery.java \
	  DefaultIORRecovery.java \
	  RebindFactory.java

OBJECTS = IORRecovery.class \
	  DefaultIORRecovery.class \
	  RebindFactory.class

Target($(OBJECTS))

BuildJavaByteCode(IORRecovery,NullParam)
BuildJavaByteCode(DefaultIORRecovery,NullParam)
BuildJavaByteCode(RebindFactory,NullParam)

Clean(*.class)
