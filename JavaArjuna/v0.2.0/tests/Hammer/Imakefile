SOURCES = Hammer.java \
	  AtomicObjectTest1.java \
	  AtomicObjectTest2.java \
	  AtomicObjectTest3.java

OBJECTS = Hammer.class \
	  AtomicObjectTest1.class \
	  AtomicObjectTest2.class \
	  AtomicObjectTest3.class

AllTarget($(OBJECTS))

BuildJavaByteCode(Hammer,NOPARAM)
BuildJavaByteCode(AtomicObjectTest1,NOPARAM)
BuildJavaByteCode(AtomicObjectTest2,NOPARAM)
BuildJavaByteCode(AtomicObjectTest3,NOPARAM)

TidyTarget(*.class *.prof out)

