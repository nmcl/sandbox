package RMISystem.rmi;

class MappingEntry
{

public MappingEntry (String objName, String jExec)
    {
	_objectName = objName;
	_javaExec = jExec;
    }

public String objectName ()
    {
	return _objectName;
    }

public String javaExec ()
    {
	return _javaExec;
    }

private String _objectName;
private String _javaExec;
    
};
@
