#if defined(NO_INLINES) && !defined(UID_CC_)

class ostream;

extern ostream& operator<< (ostream& strm, const Uid& u);

#else

#ifndef NO_INLINES
#  define INLINEF inline
#else
#  define INLINEF
#endif

INLINEF ostream& operator<<  ( ostream& strm, const Uid& u )
{
    return u.print (strm);
}

#ifdef INLINEF
#  undef INLINEF
#endif

#endif
@
