#ifndef MACRO_DEF_
#define MACRO_DEF_

#ifndef Concat
#ifdef __STDC_
#define Concat(x,y) x##y
#else
#define Concat(x,y) x/**/y
#endif
#endif

#ifndef YES
#define YES 1
#endif

#ifndef NO
#define NO  0
#endif

#ifndef NOPARAM
#define NOPARAM
#endif

#endif
