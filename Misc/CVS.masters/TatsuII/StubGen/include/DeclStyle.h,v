#ifndef DECLSTYLE_H_
#define DECLSTYLE_H_

/*
 *
 * Enums describing which particular style of type declaration to produce
 *
 */

enum DeclStyle { CLIENT_DECL = 1, SERVER_DECL = 2, 
		 SERVERIMPLEMENTATION_DECL = 4,
		 NORMAL_DECL = 8, IDL_DECL = 16, SHADOWS_DECL = 32,
                 JAVA_DECL = 64, ARJUNAII_CLIENT_DECL = 128,
                 ARJUNAII_INTF_DECL = 256, ARJUNAII_IMPL_DECL = 512 };
#endif






@


1.2
log
@first step
@
text
@d9 1
a9 1
 * $Id: DeclStyle.h,v 1.1 1997/06/09 19:52:12 nmcl Exp $
d24 2
a25 1
                 JAVA_DECL = 64, ARJUNAII_DECL = 128 };
@


1.1
log
@initial cut
@
text
@d9 1
a9 1
 * $Id: DeclStyle.h,v 1.4 1994/09/14 15:05:42 ngdp Exp $
d24 1
a24 1
                 JAVA_DECL = 64 };
