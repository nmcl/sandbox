INLINEF String FileInfo::getDname () const
{
    return fileDname;
}

INLINEF String FileInfo::getFname () const
{
    return fileFname;
}

INLINEF StubModifier FileInfo::getGeneric () const
{
    return fileGeneric;
}

INLINEF Boolean FileInfo::warningsEnabled () const
{
    return warningsOn;
}

