SOURCES = Uid.cc \
	  Semaphore.cc \
	  Utility.cc \
	  ProcessMutex.cc \
	  SharedSeg.cc \
	  Buffer.cc \
	  ObjState.cc \
	  PointerMap.cc \
	  Kubera.cc \
	  ORBInterface.cc

OBJECTS = Uid.o \
	  Semaphore.o \
	  Utility.o \
	  ProcessMutex.o \
	  SharedSeg.o \
	  Buffer.o \
	  ObjState.o \
	  PointerMap.o \
	  Kubera.o \
	  ORBInterface.o

Target($(OBJECTS))

BuildObject(Uid,NullParam,NullParam)
BuildObject(Semaphore,NullParam,NullParam)
BuildObject(Utility,NullParam,NullParam)
BuildObject(ProcessMutex,NullParam,NullParam)
BuildObject(SharedSeg,NullParam,NullParam)
BuildObject(Buffer,NullParam,NullParam)
BuildObject(ObjState,NullParam,NullParam)
BuildObject(PointerMap,NullParam,NullParam)
BuildObject(Kubera,NullParam,NullParam)
BuildObject(ORBInterface,NullParam,NullParam)

AddDependences($(SOURCES),NullParam)
