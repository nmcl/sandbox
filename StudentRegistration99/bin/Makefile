Makefiles ::
	@for i in $(SUBDIRS)""; \
	do \
	if test -d $$i ; \
 then \
	echo Making $@ in $(CURDIR)/$$i ; \
	(cd $$i; $(IMAKE) $(IMAKEFLAGS);$(MV) Makefile Makefile.sed;$(SED) -e '/^COMMENT$$/s//#/' -e '/^COMMENT /s//# /'  -e 's/\%\%/\\"/g' < Makefile.sed > Makefile;$(RM) Makefile.sed); \
	(cd $$i; $(MAKE) $(xMAKEFLAGS) MAKE="$(MAKE)" IMAKE="$(IMAKE)" MAKEDEPEND="$(MAKEDEPEND)" CPLUSPLUS=\"$(CPLUSPLUS)\" AR="$(AR)" RANLIB="$(RANLIB)" YACC="$(YACC)" LEX="$(LEX)" MV="$(MV)" CP="$(CP)" RM="$(RM)" CAT="$(CAT)" MKDIR="$(MKDIR)" CHMOD="$(CHMOD)" PURIFY="$(PURIFY)" xMAKEFLAGS="$(xMAKEFLAGS)" IMAKEFLAGS="$(IMAKEFLAGS)" MAKEDEPENDFLAGS="$(MAKEDEPENDFLAGS)" CPPFLAGS="$(CPPFLAGS)" CPLUSPLUSFLAGS="$(CPLUSPLUSFLAGS)" LDFLAGS="$(LDFLAGS)" xLIBRARIES="$(xLIBRARIES)" YACCFLAGS="$(YACCFLAGS)" LEXFLAGS="$(LEXFLAGS)" STUBFLAGS="$(STUBFLAGS)" ARFLAGS="$(ARFLAGS)" CURDIR=$(CURDIR)/$$i $@); \
	else \
	echo No such directory : $$i ;\
	fi \
	done

